{
    "id": "root",
    "type": null,
    "attributes": {
        "class": "panel panel-default",
        "style": "display: none"
    },
    "children": {
        "header": {
            "type": null,
            "attributes": {
                "class": "panel-heading"
            },
            "children": {
                "title": {
                    "type": "h2",
                    "attributes": {
                        "class": "panel-title",
                        "id": "the-panel-title"
                    },
                    "text": {
                        "reference": "var"
                    }
                }
            }
        },
        "body": {
            "type": null,
            "attributes": {
                "class": "panel-body"
            }
        },
        "footer": {
            "type": null,
            "attributes": {
                "class": "panel-footer"
            },
            "children": {
                "footerButtons": {
                    "type": null,
                    "attributes": {
                        "class": "btn-group"
                    },
                    "children": {
                        "closeBtn": {
                            "type": "button",
                            "attributes": {
                                "class": "btn btn-default"
                            },
                            "signals": {
                                "onClose": "onclick"
                            },
                            "text": "Close"
                        }
                    }
                }
            }
        }
    }
}